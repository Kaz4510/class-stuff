//to check wheather a number is prime or not.
#include <iostream>
using namespace std;
int main()
{
    int a;
    bool b;
    b=true;
    cout<<"enter a number: ";
    cin>>a;
    for(int i=2; i<=a/2; i++){
        if(a%i==0){
            b=false;
            break;
        }
    }
    cout<<b;
    return 0;
}
